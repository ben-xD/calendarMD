{
  "name": "Bed",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "both": "npm run a && npm run ios",
    "a:list": "emulator -list-avds && adb devices",
    "a:device": "emulator -avd Pixel_3_API_29",
    "a": "react-native run-android --deviceId=$ANDROID_EMULATOR",
    "android": "react-native run-android",
    "a:release": "react-native run-android --variant=release --deviceId=$ANDROID_EXTERNAL_DEVICE",
    "a:debug": "adb shell input keyevent 82 && adb shell input keyevent 61 && adb shell input keyevent 66 && adb shell input keyevent 66",
    "a:devmenu": "adb shell input keyevent 82",
    "a:bundle": "cd android && ./gradlew bundleRelease",
    "ios": "react-native run-ios --simulator=\"iPhone 11\"",
    "ios:list": "xcrun simctl list devices",
    "android-debug-cert-fingerprint": "keytool -list -v -keystore ./android/app/debug.keystore -alias androiddebugkey -storepass android -keypass android",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.7.1",
    "@react-native-community/google-signin": "^3.0.4",
    "@react-native-community/masked-view": "^0.1.5",
    "@react-native-community/netinfo": "^5.3.2",
    "@react-navigation/bottom-tabs": "^5.0.0-alpha.29",
    "@react-navigation/native": "^5.0.0-alpha.22",
    "@react-navigation/stack": "^5.0.0-alpha.53",
    "axios": "^0.19.0",
    "lottie-react-native": "^3.3.2",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-elements": "^1.2.7",
    "react-native-error-boundary": "^1.0.3",
    "react-native-gesture-handler": "^1.5.3",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-safe-area-context": "^0.6.2",
    "react-native-screens": "^2.0.0-alpha.22",
    "react-native-vector-icons": "^6.6.0",
    "styled-components": "^4.4.1",
    "typescript-plugin-styled-components": "^1.4.3"
  },
  "devDependencies": {
    "@types/gapi": "0.0.39",
    "eslint-plugin-prettier": "^3.1.2",
    "@babel/core": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "@types/jest": "^24.0.24",
    "@types/react-native": "^0.60.25",
    "@types/react-test-renderer": "16.9.1",
    "@types/styled-components": "^4.4.2",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-styled-components": "^1.10.6",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.9.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "prettier": "^1.19.1",
    "react-devtools": "^4.4.0",
    "react-test-renderer": "16.9.0",
    "typescript": "^3.7.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
